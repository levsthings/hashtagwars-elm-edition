{
  "name": "hashtagwars",
  "description": "open source front-end for hashtagwars",
  "version": "1.0.0",
  "author": "levsthings",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/levsthings/hashtagwars-elm-edition"
  },
  "scripts": {
    "cleanup": "rimraf public/",
    "dev": "webpack-dev-server --config ./src/webpack.config.dev.js --progress",
    "build": "yarn cleanup && export NODE_ENV=production || SET \"NODE_ENV=production\" && webpack --config ./src/webpack.config.prod.js --optimize-minimize --progress --hide-modules",
    "serve": "yarn build && static public",
    "test": "eslint ./src/ --color",
    "lint": "eslint --fix ./src/ --color",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": "8.2.1",
    "yarn": "0.27.5"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "elm": "^0.18.0",
    "elm-webpack-loader": "^4.3.0",
    "eslint": "^4.5.0",
    "eslint-config-monolambda": "^1.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.4.0",
    "lint-staged": "^4.0.4",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.9",
    "postcss-loader": "^2.0.5",
    "pug": "^2.0.0-rc.2",
    "pug-html-loader": "^1.1.5",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "bulma": "^0.5.1",
    "chart.js": "^2.6.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-chartjs-2": "^2.6.1",
    "react-dom": "^15.6.1"
  }
}
